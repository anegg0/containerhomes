<h1>Listing insulations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @insulations.each do |insulation| %>
      <tr>
        <td><%= link_to 'Show', insulation %></td>
        <td><%= link_to 'Edit', edit_insulation_path(insulation) %></td>
        <td><%= link_to 'Destroy', insulation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Insulation', new_insulation_path %>
