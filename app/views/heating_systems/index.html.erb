<h1>Listing heating_systems</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @heating_systems.each do |heating_system| %>
      <tr>
        <td><%= link_to 'Show', heating_system %></td>
        <td><%= link_to 'Edit', edit_heating_system_path(heating_system) %></td>
        <td><%= link_to 'Destroy', heating_system, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Heating system', new_heating_system_path %>
